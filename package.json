{
  "name": "polygon-splitter",
  "version": "0.0.3",
  "description": "Split a polygon using a polyline.",
  "main": "dist/polygonsplitter.js",
  "module": "dist/polygonsplitter.mjs",
  "unpkg": "dist/polygonsplitter.min.js",
  "scripts": {
    "bench": "npm run build && node benchmark.js",
    "build": "rollup -c",
    "build:debug": "cross-env webpack --config debug/webpack.config.js --mode production",
    "debug": "cross-env webpack serve --config debug/webpack.config.js --hot",
    "test": "ava"
  },
  "author": "Rowan Winsemius",
  "repository": {
    "url": "git+https://github.com/rowanwins/polygon-split.git",
    "type": "git"
  },
  "ava": {
    "files": [
      "test/test.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@turf/turf": "^5.1.6",
    "@vue/compiler-sfc": "^3.0.4",
    "ava": "^3.15.0",
    "benchmark": "^2.1.4",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^4.16.0",
    "eslint-config-mourner": "^2.0.3",
    "file-loader": "^6.2.0",
    "json-loader": "^0.5.7",
    "leaflet": "^1.7.1",
    "load-json-file": "^4.0.0",
    "rollup": "^2.34.2",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "^2.0.0",
    "tape": "^4.8.0",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^4.0.0-beta.0",
    "write-json-file": "^4.3.0"
  },
  "dependencies": {
    "robust-predicates": "^2.0.4"
  },
  "keywords": [
    "geojson",
    "polygon",
    "polyline",
    "split"
  ]
}
